{
  "name": "pollen-solutions/tiny-mce",
  "description": "Pollen TinyMCE Component",
  "homepage": "https://www.presstify.com/pollen-solutions/tiny-mce/",
  "keywords": [
    "presstify",
    "plugins",
    "wordpress",
    "tinyMCE",
    "wysiwyg editor"
  ],
  "time": "2021-05-27",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.presstify.com/pollen-solutions/"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "fortawesome/font-awesome": "^4.7",
    "pollen-solutions/asset": "^1.0",
    "pollen-solutions/http": "^1.0",
    "pollen-solutions/routing": "^1.0",
    "pollen-solutions/support": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\TinyMce\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\TinyMce\\Test\\": "tests"
    }
  },
  "suggest": {
    "pollen-solutions/container" : "Pollen Container Component - PSR-11 ready Dependencies Injection Container."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
